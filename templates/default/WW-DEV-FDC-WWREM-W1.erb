        <header value="HOUR    &#9;LEVEL&#9;HOST    &#9;Application&#9;Thread&#9;Class&#9;Method&#9;NDC&#9;TrackId1&#9;TrackId2&#9;Message&#13;&#10;" />
        <footer value="&#13;&#10;&#13;&#10;&#13;&#10;" />
