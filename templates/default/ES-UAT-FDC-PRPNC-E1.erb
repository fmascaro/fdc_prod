<configuration>
  <configSections>
    <section name="ContextInfo" type="eGistics.Utilities.ContextInfoSection, eGistics.GlobalConfig" />
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <ContextInfo>
    <Context>
      <add name="ES-UAT-FDC-PRPNC-E1" type="Description of PNCReturnsPreProcessor" connstring="Data Source=du-gen-sql-01;Initial Catalog=RTC_Admin3g;Integrated Security=True" />
    </Context>
  </ContextInfo>
  <log4net>
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <!--<file value="C:\temp\Fidelity\AFPPreprocessor\log\AFPPreprocessor.log" />-->
      <file value="L:\TIS\UAT\FDC\PRPNC-E1\PNCReturnsPreProcessor.log" />
      <appendToFile value="true" />
      <maximumFileSize value="1MB" />
      <maxSizeRollBackups value="100" />
      <rollingStyle value="Composite" />
      <datePattern value="yyyyMMdd" />
      <staticLogFileName value="true" />
      <filter type="log4net.Filter.LevelRangeFilter">
        <acceptOnMatch value="true" />
        <levelMin value="DEBUG" />
        <levelMax value="FATAL" />
      </filter>
      <layout type="log4net.Layout.PatternLayout">
        <header value="HOUR     LEVEL HOST    Application     Thread Class   Method   NDC   TrackId1   TrackId2   Message&#xD;&#xA;" />
        <footer value="&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;" />
        <conversionPattern value="%date{HH:mm:ss} %-5level %property{log4net:HostName} AFPPreprocessor %thread       %property{Class} %property{Method} %property{NDC} %property{TrackId1} %property{TrackId2} %message %newline" />
      </layout>
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <!-- Set the default logging level and add the active appenders Debug,Info,Warn,Error,Fatal-->
    <root>
      <level value="ALL" />
      <appender-ref ref="RollingFileAppender" />
    </root>
  </log4net>
  <appSettings>
    <add key="ServiceName" value="ES-UAT-FDC-PRPNC-E1" />
    <!--<add key="ClientSettingsProvider.ServiceUri" value="" />-->
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <system.serviceModel>
    <bindings>
      <wsHttpBinding>
        <binding name="WSHttpBinding_IEventCollection" />
      </wsHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://egi-webdev1.sdidallas.com/EventCollection/eGistics.Notification.Event.EventCollection.svc" binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_IEventCollection" contract="EventCollectionService.IEventCollection" name="WSHttpBinding_IEventCollection">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
